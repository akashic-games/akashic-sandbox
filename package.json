{
  "name": "@akashic/akashic-sandbox",
  "private": true,
  "version": "1.0.0-refine.0",
  "description": "Standalone runner for Akashic games",
  "main": "index.js",
  "scripts": {
    "start": "node lib/bootstrap.js",
    "start:dev": "node server.js",
    "clean:front": "shx rm -rf ./dist/",
    "build:server": "shx rm -rf ./lib/ && tsc -p ./src/server",
    "build:front": "npm run clean:front && npm run build:runtime && npm run build:client",
    "build:runtime": "webpack --config ./src/runtime/webpack.config.dev.js",
    "build:client": "webpack --config ./src/client/webpack.config.dev.js",
    "build:front-prod": "npm run clean:front && npm run build:runtime-prod && npm run build:client-prod",
    "build:runtime-prod": "webpack --config ./src/runtime/webpack.config.js",
    "build:client-prod": "webpack --config ./src/client/webpack.config.js"
  },
  "author": "DWANGO Co., Ltd.",
  "license": "MIT",
  "bin": {
    "akashic-sandbox": "./bin/run"
  },
  "files": [
    "bin",
    "lib",
    "dist",
    "js",
    "css",
    "views",
    "thirdparty"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/akashic-games/akashic-sandbox.git"
  },
  "publishConfig": {
    "@akashic:registry": "http://registry.npmjs.org/"
  },
  "dependencies": {
    "commander": "^2.8.0",
    "ect": "~0.5.9",
    "express": "^4.12.3",
    "mobx": "~3.3.2",
    "mobx-react": "~4.3.5",
    "mobx-react-devtools": "~4.2.15",
    "react": "~16.2.0",
    "react-dom": "~16.2.0"
  },
  "devDependencies": {
    "@types/express": "~4.0.39",
    "@types/jasmine": "~2.8.2",
    "@types/mime": "~2.0.0",
    "@types/node": "~8.0.53",
    "@types/react": "~16.0.38",
    "@types/react-dom": "~16.0.4",
    "@types/supertest": "~2.0.4",
    "awesome-typescript-loader": "~3.4.0",
    "browserify": "^13.0.0",
    "css-loader": "~0.28.7",
    "extract-text-webpack-plugin": "~3.0.2",
    "file-loader": "~1.1.5",
    "font-awesome": "~4.7.0",
    "jasmine": "^2.1.1",
    "shx": "~0.2.2",
    "style-loader": "~0.19.0",
    "supertest": "^1.2.0",
    "ts-loader": "~3.1.1",
    "tslint": "^5.4.3",
    "typescript": "~2.6.1",
    "uglify-js": "2.7.5",
    "uglify-save-license": "^0.4.1",
    "url-loader": "~0.6.2",
    "webpack": "~3.8.1",
    "webpack-dev-server": "~2.9.4"
  }
}
